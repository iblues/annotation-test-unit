{
    "name": "iblues/annotation-test-unit",
    "description": " Base on annotation's and laravel phpunit.\n一个基于注解和laravel单元测试的 自动化测试包",
    "authors": [
        {
            "name": "Blues",
            "email": "I@iblues.name"
        }
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Iblues\\AnnotationTestUnit\\": "src"
        }
    },
    "require": {
        "php": ">=7.0",
        "doctrine/annotations": "^1.8"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Iblues\\AnnotationTestUnit\\ServiceProvider"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": ">7.0"
    }
}
